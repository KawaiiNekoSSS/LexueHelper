<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.localSubmitController">
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" percentWidth="15.0" prefWidth="100.0" />
      <ColumnConstraints maxWidth="100.0" minWidth="10.0" percentWidth="40.0" prefWidth="100.0" />
      <ColumnConstraints maxWidth="100.0" minWidth="10.0" percentWidth="38.5" prefWidth="100.0" />
      <ColumnConstraints maxWidth="0.0" minWidth="0.0" prefWidth="0.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="70.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="20.0" prefHeight="30.0" />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextArea fx:id="codeText" prefHeight="500.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label prefHeight="20.0" prefWidth="170.0" text="您可以将代码粘贴到这里" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
      <GridPane GridPane.columnIndex="2" GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ChoiceBox prefHeight="41.0" prefWidth="162.0" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM" />
            <Label prefHeight="20.0" prefWidth="110.0" text="请选择您的语言" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
         </children>
      </GridPane>
      <Button onAction="#submit" fx:id="submit" mnemonicParsing="false" prefHeight="42.0" prefWidth="80.0" text="提交" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
      <Button onAction="#goBack" fx:id="back" mnemonicParsing="false" prefHeight="35.0" prefWidth="58.0" text="返回" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
   </children>
</GridPane>
